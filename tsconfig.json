{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "rootDir": "./node-assignment/src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["node-assignment/src/**/*"],
  "exclude": ["node_modules", "dist"]
}
